{
  "name": "conditional-injector",
  "version": "1.0.4",
  "description": "Typescript dependency injectional based on conditional expressions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "node_modules/.bin/tslint --project tsconfig.json --force",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopidio/conditional-injector.git"
  },
  "keywords": [
    "typescript",
    "dependency-injection",
    "predicate",
    "injection",
    "smart",
    "conditional",
    "injector",
    "javascript",
    "javascript-library"
  ],
  "author": "Virgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lopidio/conditional-injector/issues"
  },
  "homepage": "https://github.com/lopidio/conditional-injector#readme",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.6",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.5",
    "ts-node": "^4.1.0",
    "tsd": "^0.6.5",
    "tslint": "^5.10.0",
    "typescript": "^2.7.2"
  },
  "bin": {
    "json-placeholder-replacer": "dist/index.js"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
