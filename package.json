{
  "name": "conditional-injector",
  "version": "1.0.15",
  "description": "Typescript dependency injectional based on conditional expressions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "node_modules/.bin/jest",
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "codeCoverage": "jest --silent --coverage",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lopidio/conditional-injector.git"
  },
  "keywords": [
    "typescript",
    "dependency-injection",
    "predicate",
    "injection",
    "smart",
    "conditional",
    "injector",
    "javascript",
    "javascript-library"
  ],
  "author": "Virgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lopidio/conditional-injector/issues"
  },
  "homepage": "https://github.com/lopidio/conditional-injector#readme",
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/conditional-injector.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 90,
        "functions": 95,
        "lines": 95
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
